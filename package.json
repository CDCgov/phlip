{
  "name": "esquire-frontend",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "description": "Esquire Frontend",
  "scripts": {
    "build:image:dev": "docker-compose build dev",
    "build:image:prod": "docker-compose build prod",
    "start": "docker-compose up --build dev",
    "serve": "docker-compose up --build -d prod",
    "test": "docker-compose run dev npm run test:local",
    "test:watch": "docker-compose run dev npm run test:watch:local",
    "start:local": "node scripts/start.js",
    "serve:local": "http-server ./dist -p 5000",
    "test:local": "jest --coverage",
    "test:watch:local": "jest --verbose --watch",
    "prebuild": "npm run clean",
    "build": "node scripts/build.js",
    "clean": "rm -rf ./dist/"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/config/tests/setupFiles.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ]
  },
  "repository": "git",
  "author": "Kristin Muterspaw",
  "contributors": [
    "Michael Ta",
    "Kristin Muterspaw"
  ],
  "dependencies": {
    "axios": "^0.16.2",
    "babel-runtime": "6.26.0",
    "chalk": "^2.1.0",
    "dotenv": "^4.0.0",
    "http-server": "^0.10.0",
    "prop-types": "15.5.10",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-router": "4.1.2",
    "react-router-dom": "4.1.2"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.0",
    "babel-loader": "7.1.1",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-plugin-transform-object-assign": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react-app": "3.0.2",
    "babel-preset-stage-0": "6.24.1",
    "cli-spinner": "^0.2.6",
    "compression-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "eslint": "^4.6.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.0.1",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "react-hot-loader": "^1.3.1",
    "react-test-renderer": "^15.6.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.2",
    "webpack-dev-server": "^2.6.1"
  },
  "optionalDependencies": {
    "fsevents": "*"
  }
}
