    "image-compressor": "1.2.0",
    "jsonwebtoken": "8.3.0",
    "jwt-decode": "2.2.0",
    "material-ui": "1.0.0-beta.25",
    "material-ui-pickers": "1.0.0-beta.12",
    "mdi-material-ui": "3.0.1",
    "moment": "2.22.2",
    "passport": "0.4.0",
